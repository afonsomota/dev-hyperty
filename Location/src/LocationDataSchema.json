{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "Location",
    "type": "object",
    "properties": {
      "geoposition": {
        "type": "object",
        "properties": {
            "coords": {
                "type": "object",
                "properties":{
                    "latitude":{
                        "type": "number"
                    },
                    "longitude":{
                        "type": "number"
                    },
                    "altitude":{
                        "type": "number"
                    },
                    "accuaracy":{
                        "type": "number"
                    },
                    "altitudeAccuracy":{
                        "type": "number"
                    },
                    "heading":{
                        "type": "number"
                    },
                    "speed":{
                        "type": "number"
                    }
                }
            },
            "timestamp":{
                "type": "number"
            }
        }
      }
    }
}
